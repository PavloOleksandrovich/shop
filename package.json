  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "scripts": {
    "setup": "yarn && yarn setup:api && yarn setup:web",
    "setup:api": "yarn --cwd api",
    "setup:web": "yarn --cwd web",
    "dev": "concurrently \"yarn dev:api\" \"yarn dev:web\" --kill-others",
    "dev:api": "yarn --cwd api dev",
    "dev:web": "yarn --cwd web start"
  }
}
